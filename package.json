{
  "name": "transfermyti.me",
  "version": "1.2.0",
  "description": "Web app to transfer data between time tracking tools.",
  "repository": "https://github.com/mikerourke/transfermyti.me.git",
  "author": "Mike Rourke <mike.w.rourke@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "yarn tidy",
    "build": "parcel build ./src/index.html --out-dir public --no-source-maps --no-cache",
    "postbuild": "cp ./resources/* public",
    "clockify:delete": "node scripts/manageClockify.mjs delete",
    "clockify:write": "node scripts/manageClockify.mjs write",
    "lint": "eslint --ext \".js,.ts,.tsx\" \"{scripts,src}/**/*.{js,mjs,ts,tsx}\"",
    "prettier": "prettier --write \"{scripts,src,server}/**/*.{js,ts,tsx,css,html}\"",
    "prestart:web": "mkdir -p ./dist && cp ./resources/* dist",
    "start:web": "parcel serve src/index.html --out-dir ./dist --public-url / --port 3000 --target browser",
    "start:server": "nodemon server/index.mjs",
    "start": "USE_LOCAL_API=true concurrently --kill-others \"yarn start:server\" \"yarn start:web\"",
    "test": "NODE_ENV=test node scripts/runTests.mjs --env=jsdom --config jest.config.json",
    "tidy": "rimraf ./.cache && rimraf ./public && rimraf ./dist",
    "typecheck": "tsc -p tsconfig.json --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "color": "^3.1.2",
    "connected-react-router": "^6.6.1",
    "cuid": "^2.1.8",
    "date-fns": "^2.8.1",
    "fetch-intercept": "^2.3.1",
    "history": "4.10.1",
    "qs": "^6.9.1",
    "ramda": "^0.27.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.1.2",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "store": "^2.0.12",
    "typesafe-actions": "^5.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-syntax-json-strings": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-env": "7.13.8",
    "@babel/preset-typescript": "^7.9.0",
    "@emotion/babel-plugin": "^11.0.0",
    "@emotion/jest": "^11.3.0",
    "@testing-library/jest-dom": "^5.1.0",
    "@testing-library/react": "^12.1.2",
    "@types/color": "^3.0.1",
    "@types/jest": "^27.0.3",
    "@types/jest-in-case": "^1.0.1",
    "@types/qs": "^6.9.0",
    "@types/ramda": "^0.27.3",
    "@types/react": "^17.0.7",
    "@types/react-dom": "^17.0.5",
    "@types/react-helmet": "^6.0.0",
    "@types/react-redux": "^7.0.3",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-transition-group": "^4.2.3",
    "@types/redux-mock-store": "^1.0.1",
    "@types/store": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-jest": "^27.3.1",
    "babel-plugin-jsx-pragmatic": "^1.0.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^6.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-unicorn": "^39.0.0",
    "express": "^4.16.4",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.1",
    "jest-in-case": "^1.0.2",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.21",
    "morgan": "^1.9.1",
    "node-fetch": "^3.1.0",
    "nodemon": "^2.0.2",
    "parcel-bundler": "1.12.3",
    "picocolors": "^1.0.0",
    "prettier": "^2.0.2",
    "promise-throttle": "^1.0.1",
    "redux-mock-store": "^1.5.4",
    "redux-saga-test-plan": "^4.0.4",
    "rimraf": "^3.0.0",
    "typescript": "^4.5.2",
    "yargs": "^17.0.1"
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": false,
    "trailingComma": "all"
  }
}
